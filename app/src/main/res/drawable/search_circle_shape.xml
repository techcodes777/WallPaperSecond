            <gradient android:startColor="#F5F5F9"
                      android:endColor="#DADFE7"/>
        </shape>
    </item>
    <item
        android:top="3dp"
        android:right="3dp"
        android:bottom="3dp"
        android:left="3dp">
        <shape
            android:shape="oval">
